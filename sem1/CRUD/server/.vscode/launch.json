{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Server : Debug",
      "program": "${workspaceFolder}/debug.js",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "autoAttachChildProcesses": true,
      "restart": true,
      "sourceMaps": true,
      "port": 9229,
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Server : Release",
      "program": "${workspaceFolder}/release.js",
      "console": "integratedTerminal",
      "autoAttachChildProcesses": true,
      "restart": true,
      "sourceMaps": true,
      "port": 9229,
      "skipFiles": [
        "<node_internals>/**"
      ]
    }
  ]
}
